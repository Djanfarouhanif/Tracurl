{
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/dist/landingpage.html" },
    { "src": "/auth", "dest": "/dist/dist/accuille.html" },
    { "src": "/dashboard", "dest": "/dist/index.html" }
  ],
  "outputDirectory": "dist"
}
